//检查登录  ---------------------------
function CheckLogin() {
	$.get(ElecfansApi_checklogin, function(data, textStatus) {
		if (data != "") {
			EchoLoginInfo(data);
			CheckEmailInfo(data);
			//2015-03-30 首页添加VIP状态标志
			data = $.parseJSON(data);
			if (data.vip === 1) {
				$('.u-info').append('<span class="myvip" style="color:#ec7600;font-style:italic;"> VIP </span>');
			}
		} else {
			remainLog();
			$('#login-info').show();
		}
	});
}
