$(function() {
	var AdHtmlRight = $('#AdHtmlRight');
	if ($(".col-sub").length !== 0 && AdHtmlRight.length !== 0) {
		var tiHtml = '<div class="tab-content tab-show ti" id="openx-ti-company"></div>';
		AdHtmlRight.before(tiHtml);
	}
});

