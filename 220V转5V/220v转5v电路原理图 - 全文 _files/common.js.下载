//微信
$(window).scroll(function() {
	var scrollTop = $(window).scrollTop();
	if (($('#pfwx').length > 0) || (scrollTop < 0)) {
		function resizewx() {
			var pwidth = $(window).width();
			if (pwidth < 1110) {
				$('#pfwx').css('display', 'none');
			} else {
				var rwidth = 1020 + (pwidth - 1000) / 2;
				$('#pfwx').css('left', rwidth);
				/*首页二维码暂时隐藏*/
				$('#pfwx').css('display', 'none');
			}
		}
		resizewx();
		$(window).resize(function() {
			resizewx();
		});
	}
})



